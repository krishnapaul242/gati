{
  "name": "@gati/registry",
  "version": "1.0.0",
  "description": "Official artifact registry for the Gati ecosystem",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "gati dev",
    "build": "gati build",
    "start": "node dist/app.js",
    "test": "vitest",
    "test:module": "vitest --grep",
    "db:migrate": "gati db migrate",
    "db:seed": "gati db seed",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gati-framework/core": "workspace:*",
    "@gati-framework/runtime": "workspace:*",
    "@gati-framework/types": "workspace:*",
    "@aws-sdk/client-s3": "^3.450.0",
    "@elastic/elasticsearch": "^8.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "stripe": "^14.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
