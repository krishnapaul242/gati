  },
  "bugs": {
    "url": "https://github.com/krishnapaul242/gati/issues"
  },
  "homepage": "https://github.com/krishnapaul242/gati#readme",
  "dependencies": {
    "@gati-framework/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
