{
  "name": "@gati-framework/runtime",
  "version": "1.0.0",
  "description": "Gati runtime execution engine for running handler-based applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
