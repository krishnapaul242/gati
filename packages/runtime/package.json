{
  "name": "@gati-framework/runtime",
  "version": "2.0.5",
  "description": "Gati runtime execution engine for running handler-based applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app-core": {
      "types": "./dist/app-core.d.ts",
      "import": "./dist/app-core.js"
    },
    "./handler-engine": {
      "types": "./dist/handler-engine.d.ts",
      "import": "./dist/handler-engine.js"
    },
    "./route-manager": {
      "types": "./dist/route-manager.d.ts",
      "import": "./dist/route-manager.js"
    },
    "./route-parser": {
      "types": "./dist/route-parser.d.ts",
      "import": "./dist/route-parser.js"
    },
    "./module-registry": {
      "types": "./dist/module-registry.d.ts",
      "import": "./dist/module-registry.js"
    },
    "./module-loader": {
      "types": "./dist/module-loader.d.ts",
      "import": "./dist/module-loader.js"
    },
    "./global-context": {
      "types": "./dist/global-context.d.ts",
      "import": "./dist/global-context.js"
    },
    "./local-context": {
      "types": "./dist/local-context.d.ts",
      "import": "./dist/local-context.js"
    },
    "./context-manager": {
      "types": "./dist/context-manager.d.ts",
      "import": "./dist/context-manager.js"
    },
    "./request": {
      "types": "./dist/request.d.ts",
      "import": "./dist/request.js"
    },
    "./response": {
      "types": "./dist/response.d.ts",
      "import": "./dist/response.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./types/module": {
      "types": "./dist/types/module.d.ts",
      "import": "./dist/types/module.js"
    },
    "./types/context": {
      "types": "./dist/types/context.d.ts",
      "import": "./dist/types/context.js"
    },
    "./timescape/registry": {
      "types": "./dist/timescape/registry.d.ts",
      "import": "./dist/timescape/registry.js"
    },
    "./timescape/timeline-store": {
      "types": "./dist/timescape/timeline-store.d.ts",
      "import": "./dist/timescape/timeline-store.js"
    },
    "./timescape/types": {
      "types": "./dist/timescape/types.d.ts",
      "import": "./dist/timescape/types.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "gati",
    "runtime",
    "api",
    "framework",
    "handlers",
    "cloud-native"
  ],
  "author": "Krishna Paul",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/krishnapaul242/gati.git",
    "directory": "packages/runtime"
  },
  "bugs": {
    "url": "https://github.com/krishnapaul242/gati/issues"
  },
  "homepage": "https://github.com/krishnapaul242/gati#readme",
  "dependencies": {
    "@gati-framework/contracts": "workspace:*",
    "@gati-framework/core": "^0.4.1",
    "@opentelemetry/api": "^1.9.0",
    "pino": "^9.5.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "fast-check": "^4.3.0",
    "pino-pretty": "^13.1.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
