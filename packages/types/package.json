{
  "name": "@gati-framework/types",
  "version": "1.0.1",
  "description": "Gati Type System - TypeScript-first branded types and schema system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./gtype": {
      "types": "./dist/gtype.d.ts",
      "import": "./dist/gtype.js"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "import": "./dist/registry.js"
    },
    "./brands": {
      "types": "./dist/brands/index.d.ts",
      "import": "./dist/brands/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "gati",
    "types",
    "branded-types",
    "validation",
    "schema",
    "typescript"
  ],
  "author": "Krishna Paul",
  "license": "MIT",
  "peerDependencies": {
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
